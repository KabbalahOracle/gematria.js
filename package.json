      "firefox/16..latest",
      "safari/latest",
      "opera/11.0..latest",
      "iphone/6",
      "ipad/6",
      "android-browser/latest"
    }
  },
  "author": "Akiva Levy <akiva@sixthirteen.co> (http://sixthirteen.co/)",
  "license": "MIT",
  "devDependencies": {
    "covert": "^1.0.1",
    "faucet": "0.0.1",
    "tape": "^3.0.3"
  }
}
